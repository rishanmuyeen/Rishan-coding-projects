const [count,setCount] = useState(0);
  const [text,setText] = useState("");
  const [show,setShow] = useState(true);

  const Increase = () =>{
    setCount(count + 1);
  }

  const Decrease = () =>{
    setCount(count-1);
  }

  const Reset = () =>{
    setCount(0);
  }

  const InputChange = (event) =>{
    setText(event.target.value);
  }
  const changeShow = ()=>{
    setShow(!show)